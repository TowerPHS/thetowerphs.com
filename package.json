{
  "private": true,
  "name": "thetowerphs.com",
  "description": "The website for Princeton High School's newspaper club, The Tower",
  "license": "MIT",
  "scripts": {
    "build:remix": "remix build",
    "build:css": "sass styles:app/styles/ --style=compressed",
    "build": "run-s build:css build:remix",
    "dev:remix": "remix dev",
    "dev:css": "sass styles/:app/styles/ --watch ",
    "dev": "run-p dev:css dev:remix",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@remix-run/vercel": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.8.1",
    "sass": "^1.49.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
