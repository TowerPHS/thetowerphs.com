generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model article {
  id        String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  author    String
  content   String
  date      DateTime @db.Date
  published Boolean
  section   String
  title     String
}
